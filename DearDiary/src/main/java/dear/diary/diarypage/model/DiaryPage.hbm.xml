<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dear.diary.diarypage.model">
    <class name="DiaryPage" table="DIARY_PAGES">
        <id name="recordId" column="RECORD_ID">
            <generator class="native"/>
        </id>
        <property name="diaryId" column="DIARY_ID"  />
        <property name="pageDate" column="PAGE_DATE" />
        <property name="contentLanguage" column="CONTENT_LANGUAGE" />
        <property name="content" column="CONTENT" type="text" />
    </class> 
</hibernate-mapping>