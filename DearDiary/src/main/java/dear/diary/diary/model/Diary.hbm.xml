<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dear.diary.diary.model">
    <class name="Diary" table="DIARIES">
        <id name="diaryId" column="DIARY_ID">
            <generator class="native"/>
        </id>
        <property name="userId" column="USER_ID"/>
        <property name="diaryName" column="DIARY_NAME" />
        <set name="diaryPages" table="DIARY_PAGES" inverse="true" lazy="true" fetch="select">
        	<key column="DIARY_ID" not-null="true"></key>
        	<one-to-many class="dear.diary.diarypage.model.DiaryPage"/>
        </set>
    </class> 
</hibernate-mapping>